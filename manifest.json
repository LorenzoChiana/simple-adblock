{
    "name": "dyi-adblocker",
    "version": "1.0",
    "description": "simple adblocker",
    "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
    "background": {
      "scripts": ["blocked_domains.js", "background.js"]
    },
    "browser_action": {
      "default_popup": "adblock.html"
    },
    "manifest_version": 2
}